{
  "compilerOptions": {
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "sourceMap": true,
    "target": "es2020",
    "strictNullChecks": true,
    "moduleResolution": "node",
    "module": "esnext",
	"allowSyntheticDefaultImports": true
  },
  "exclude": [
    "node_modules"
  ]
}
